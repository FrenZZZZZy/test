{
   "action": {
      "default_icon": "icon.png"
   },
   "content_scripts": [
      {
         "include_globs": [
            "*://*/*"
         ],
         "js": [
            "inject.js"
         ],
         "matches": [
            "*://*/*"
         ],
         "run_at": "document_start"
      }
   ],
   "host_permissions": [
      "https://*.innogamescdn.com/",
      "https://*.forgeofempires.com/",
      "*://*/*"
   ],
   "description": "ForgeTools is a extension which gives you the ability to simplify actions (like motivating, fighting, negotiating, sniping and more)",
   "icons": {
      "128": "icon.png",
      "16": "icon.png",
      "48": "icon.png"
   },
   "manifest_version": 3,
   "name": "ForgeTools",
   "permissions": [
      "storage",
      "webRequest",
      "declarativeNetRequest",
      "declarativeNetRequestFeedback"
   ],
   "key": "",
   "version": "5.0",
   "web_accessible_resources": [
      {
         "resources": [
            "config/*.json",
            "config/language/*.json",
            "html/*.html",
            "html/settings/*.html",
            "thirdParty/*.js",
            "js/*.js",
            "css/*.css",
            "images/*.png",
            "images/*.cur",
            "negotiation_tables/*.zip"
         ],
         "matches": [
            "<all_urls>"
         ]
      }
   ],
   "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self';"
   }
}